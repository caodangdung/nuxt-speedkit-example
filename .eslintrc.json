{
  "root": true,
  "parserOptions": {
    "parser": "@babel/eslint-parser",
    "sourceType": "module",
    "requireConfigFile": false
  },
  "extends": [
    "@nuxtjs",
    "plugin:nuxt/recommended"
  ],
  "plugins": [],
  "rules": {
    "vue/multi-word-component-names": "off",
    "semi": [
      2,
      "always"
    ]
  }
}
